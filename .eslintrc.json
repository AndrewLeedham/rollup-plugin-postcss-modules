{
    "extends": "flying-sheep",
    "parser": "typescript-eslint-parser",
    "plugins": ["typescript"],
    "rules": {
        "no-console": 0,
        "no-undef": 0,                  // typescript will complain itself
        "comma-dangle": [2, "only-multiline"],
        "no-unused-vars": 0,
        "typescript/no-unused-vars": 2  // mark vars as used
    }
}